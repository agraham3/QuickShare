DROP DATABASE IF EXISTS quickShare;
CREATE DATABASE quickShare;
USE quickShare;
CREATE TABLE User
{
ID int NOT NULL AUTO_INCREMENT,
PRIMARY KEY (ID),
username NOT NULL varchar(15),
password NOT NULL varchar(25)
};
CREATE TABLE Album
{
ID int NOT NULL AUTO_INCREMENT,
PRIMARY KEY (ID),
user_id int,
name NOT NULL varchar(40)
};
CREATE TABLE Image
{
ID int NOT NULL AUTO_INCREMENT,
PRIMARY KEY (ID),
user_id int,
name NOT NULL varchar(40)
};
